t=int(input())
for i in range(t):
    n,k=map(int,input().split())
    a=list(map(int,input().split()))
    b=list(map(int,input().split()))
    for I in range(k):
        ma=min(a)
        mb=max(b)
        if ma<mb:
            x=a.index(ma)
            y=b.index(mb)
            a[x],b[y]=b[y],a[x]
        else:
            break
    print(sum(a))